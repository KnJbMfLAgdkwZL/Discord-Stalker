        <a href="/guild_channels?id={{guild.id}}">channels</a> |
        <a href="/guild_roles?id={{guild.id}}">roles</a> |
        <a href="/guild_systemChannel?id={{guild.id}}">systemChannel</a>
        -->
    </div>

</div>

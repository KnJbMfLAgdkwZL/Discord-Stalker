<div class="guilds">
    {{#each guilds}}
        <div class="guild">

            <div class="guild_head">
                <a href="/guild?id={{id}}" class="aguild">
                <img class="head_ico" src='https://cdn.discordapp.com/icons/{{id}}/{{icon}}.webp'/>
                </a>
                <div class="guild_head_title">
                    <div class="guild_name">{{name}}</div>
                    <div class="guild_id">{{id}}</div>
                </div>

            </div>

            {{#each channels}}
                <div class="guild_chanel">
                    <div class="chanel_head">
                        <div class="chanel_name">{{channel_name}} </div>
                        |
                        <div class="chanel_id"> {{channel_id}}</div>
                    </div>
                    {{#each users}}
                        <div class="chanel_user friend_{{friend}}">

                            <a href="/friend_info?id={{user_id}}" class="id_a">
                            {{#if avatar}}
                                <img class="user_avatar"
                                     src='https://cdn.discordapp.com/avatars/{{user_id}}/{{avatar}}.png?size=128'/>
                            {{else}}
                                <div class="user_avatar no_avatar"></div>
                            {{/if}}
                            </a>

                            <div class="user_title_wraper">
                                {{#if nickname}}
                                    <div class="user_nickname">{{nickname}}</div>
                                {{/if}}
                                <div class="user_username">{{#if bot}}[BOT]{{/if}}{{username}}<div class="discriminator">#{{discriminator}}</div>
                                    {{#if muted}}
                                        <svg name="Nova_MicrophoneMute" class="icon-3BcwQu" aria-hidden="false"
                                             width="24" height="24" viewBox="0 0 24 24">
                                            <path d="M6.7 11H5C5 12.19 5.34 13.3 5.9 14.28L7.13 13.05C6.86 12.43 6.7 11.74 6.7 11Z"
                                                  fill="currentColor"></path>
                                            <path d="M9.01 11.085C9.015 11.1125 9.02 11.14 9.02 11.17L15 5.18V5C15 3.34 13.66 2 12 2C10.34 2 9 3.34 9 5V11C9 11.03 9.005 11.0575 9.01 11.085Z"
                                                  fill="currentColor"></path>
                                            <path d="M11.7237 16.0927L10.9632 16.8531L10.2533 17.5688C10.4978 17.633 10.747 17.6839 11 17.72V22H13V17.72C16.28 17.23 19 14.41 19 11H17.3C17.3 14 14.76 16.1 12 16.1C11.9076 16.1 11.8155 16.0975 11.7237 16.0927Z"
                                                  fill="currentColor"></path>
                                            <path d="M21 4.27L19.73 3L3 19.73L4.27 21L8.46 16.82L9.69 15.58L11.35 13.92L14.99 10.28L21 4.27Z"
                                                  fill="currentColor"></path>
                                        </svg>
                                    {{/if}}
                                    {{#if deafened}}
                                        <svg name="Nova_HeadsetDeafen" class="icon-3BcwQu" aria-hidden="false"
                                             width="24" height="24" viewBox="0 0 24 24">
                                            <path d="M6.16204 15.0065C6.10859 15.0022 6.05455 15 6 15H4V12C4 7.588 7.589 4 12 4C13.4809 4 14.8691 4.40439 16.0599 5.10859L17.5102 3.65835C15.9292 2.61064 14.0346 2 12 2C6.486 2 2 6.485 2 12V19.1685L6.16204 15.0065Z"
                                                  fill="currentColor"></path>
                                            <path d="M19.725 9.91686C19.9043 10.5813 20 11.2796 20 12V15H18C16.896 15 16 15.896 16 17V20C16 21.104 16.896 22 18 22H20C21.105 22 22 21.104 22 20V12C22 10.7075 21.7536 9.47149 21.3053 8.33658L19.725 9.91686Z"
                                                  fill="currentColor"></path>
                                            <path d="M3.20101 23.6243L1.7868 22.2101L21.5858 2.41113L23 3.82535L3.20101 23.6243Z"
                                                  fill="currentColor"></path>
                                        </svg>
                                    {{/if}}
                                </div>
                                <div class="user_id">{{user_id}}</div>
                            </div>

                        </div>

                    {{/each}}

                </div>

            {{/each}}


        </div>
    {{/each}}
</div>

<style>
    .icon-3BcwQu {
        width: 14px;
        height: 14px;
        color: white;
    }

    .guilds {
        font-size: 15px;
    }

    img.head_ico {
        display: inline-block;
        height: 48px;
        width: 48px;
        border-radius: 5px;
    }

    .guild_head_title {
        margin-left: 5px;
        font-size: 17px;
    }

    .guild_name {
        font-weight: bold;
    }

    .guild_id {
        font-style: italic;
    }

    .guild_chanel {
        margin-bottom: 5px;
    }

    .chanel_head {
        margin-bottom: 3px;
    }

    .chanel_name {
        display: inline-block;
        font-weight: bold;
    }

    .chanel_id {
        display: inline-block;
        font-style: italic;
    }

    .user_nickname {
        color: gray;
        display: inline-block;
    }

    .user_username {
        display: inline-block;
    }

    img.user_avatar {
        height: 35px;
        border-radius: 90px;
    }

    .no_avatar {
        display: inline-block;
        width: 35px;
        height: 35px;
        background: gray;
        border-radius: 90px;
    }

    .user_title_wraper {
        display: inline-block;
        position: absolute;
        margin-left: 5px;
        margin-top: 2px;
    }

    .user_id {
        font-style: italic;
        font-size: 14px;
    }

    .chanel_user {
        margin-bottom: 1px;
        margin-left: 5px;
        font-size: 14px;
    }

    .friend_true {
        background: #EAEAEA;
    }

    .discriminator {
        display: inline-block;
        color: gray;
    }

</style>
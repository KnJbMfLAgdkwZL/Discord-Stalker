<div class="guild_channels">

    <div class="VoiceChannels">
        <h3>Voice Channels Length: {{VoiceChannels.length}}</h3>
        {{#each VoiceChannels}}
            <div class="VoiceChannel">
                <div class="title">
                    <div class='buffer'>
                        <svg name="Speaker" class="icon-1_QxNX" aria-hidden="false" width="24" height="24" viewBox="0 0 24 24"><path fill="currentColor" fill-rule="evenodd" clip-rule="evenodd" d="M11.383 3.07904C11.009 2.92504 10.579 3.01004 10.293 3.29604L6 8.00204H3C2.45 8.00204 2 8.45304 2 9.00204V15.002C2 15.552 2.45 16.002 3 16.002H6L10.293 20.71C10.579 20.996 11.009 21.082 11.383 20.927C11.757 20.772 12 20.407 12 20.002V4.00204C12 3.59904 11.757 3.23204 11.383 3.07904ZM14 5.00195V7.00195C16.757 7.00195 19 9.24595 19 12.002C19 14.759 16.757 17.002 14 17.002V19.002C17.86 19.002 21 15.863 21 12.002C21 8.14295 17.86 5.00195 14 5.00195ZM14 9.00195C15.654 9.00195 17 10.349 17 12.002C17 13.657 15.654 15.002 14 15.002V13.002C14.551 13.002 15 12.553 15 12.002C15 11.451 14.551 11.002 14 11.002V9.00195Z"></path></svg>
                    </div>

                    <div class="c_name">{{name}}</div>
                    |
                    <div class="c_id">{{id}}</div>
                    |
                    <div class="c_Members_Length">Members Length: {{members.length}}</div>
                </div>
                <div class="members">
                    {{#each members}}
                        <div class="chanel_user friend_{{friend}}">
                            <a href="/friend_info?id={{id}}" class="id_a">
                            {{#if avatar}}
                                <img class="user_avatar" src='{{AVATAR}}'/>
                            {{/if}}
                            </a>
                            <div class="user_title_wraper">
                                <div class="user_username">
                                    {{#if bot}}[BOT]{{/if}}
                                    {{tag}}
                                </div>
                                <div class="user_id">{{id}}</div>
                            </div>
                        </div>
                    {{/each}}
                </div>
            </div>
        {{/each}}
    </div>
    <hr/>
    <div class="TextChannels">

        <h3>Text Channels Length: {{TextChannels.length}}</h3>
        {{#each TextChannels}}
            <div class="TextChannels">

                <div class="title">
                    <div class="c_name">{{name}}</div>
                    |
                    <div class="c_id">
                        <a href="/guild_text_channel?id={{id}}">
                            {{id}}
                        </a>
                    </div>
                    |
                    <div class="c_id">{{type}}</div>
                </div>

            </div>
        {{/each}}

    </div>

</div>

<style>

    .guild_channels{
        background-color: #2f3136;
    }
    .buffer{
        display: inline-block;
        margin-right: 6px;
    }
    .title{
        height: 34px;
        overflow: hidden;
        margin: 1px 0 1px 8px;
    }
    .members {
        padding-left: 36px;
    }
    .c_name {
        display: inline-block;
        font-weight: bold;
        font-size: 16px;
        line-height: 20px;
        font-weight: 500;
        text-overflow: ellipsis;
        margin-top: 2px;
        margin-bottom: 5px;
        margin-right: 25px;
    }

    .c_id {
        display: inline-block;
        font-size: 15px;
    }

    .c_Members_Length {
        display: inline-block;
        font-size: 15px;
    }

    .user_username {
        display: inline-block;
        margin-right: 15px;
        color: #8e9297;
    }

    img.user_avatar {
        display: inline-block;
        height: 24px;
        width: 24px;
        border-radius: 90px;
        margin: 3px 8px 3px 8px;
    }

    .user_title_wraper {
        display: inline-block;
        display: flex;

    }

    .user_id {
        font-style: italic;
        font-size: 14px;
    }

    .chanel_user {
        font-size: 14px;
        margin-top: 1px;
        margin-bottom: 1px;
    }

</style>